package sec03.exam04;

public class ByteOperationExamlpe {

	public static void main(String[] args) {
		byte value1 = 10;
		int value2 = 100;
		long value3 = 1000L;
		//long result = (long)value1 + (long)value2 + (long)value3;
		long result = value1 + value2 + value3;
		//byte result = value1 + value2 + value3; 위는 long로 전환해줘서 에러가 뜨지 않음
		System.out.println(result);
	}

}
